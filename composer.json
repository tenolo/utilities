{
    "name": "tenolo/utilities",
    "description": "Only some basic utility classes.",
    "keywords": [
        "base",
        "utilities",
        "utility",
        "util",
        "utils",
        "tenolo"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Utilities\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Utilities\\Utils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.0": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
