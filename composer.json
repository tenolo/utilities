{
    "name": "tenolo/utilities",
    "description": "Only some basic utility classes.",
    "keywords": [
        "base",
        "utilities",
        "utility",
        "util",
        "utils",
        "tenolo"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "doctrine/common": "~2.6",
        "symfony/options-resolver": "~2.8|~3.0",
        "hackzilla/password-generator": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Utilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Utilities\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.0": "1.0-dev",
            "dev-release/1.1": "1.1-dev",
            "dev-release/1.2": "1.2-dev",
            "dev-release/1.3": "1.3-dev",
            "dev-release/1.4": "1.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
